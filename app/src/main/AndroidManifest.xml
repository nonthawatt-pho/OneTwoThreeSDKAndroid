<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ccpp.onetwothreedemo">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
            android:name=".DemoApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_123_app"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_123_app_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.OneTwoThreeAndroid">
        <activity android:name="com.ccpp.onetwothreedemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="onetwothreeapp" />
            </intent-filter>
        </activity>
    </application>

</manifest>